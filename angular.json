{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {"glob": "jquery.minicolors.png", 
              "input": "./node_modules/@claviska/jquery-minicolors",
              "output": "/"}
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@claviska/jquery-minicolors/jquery.minicolors.css",
              "node_modules/angular-tree-component/dist/angular-tree-component.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css"
            ],
            "scripts": [
              "src/flot/jquery.js",
              "src/flot/jquery.mousewheel.js",
              "src/flot/jquery.flot.js",
              "src/flot/jquery.flot.resize.js",
              "src/flot/jquery.flot.navigate.js",
              "src/flot/jquery.flot.fillarea.js",
              "src/flot/jquery.flot.crosshair.js",
              "src/flot/jquery.flot.time.js",
              "src/flot/jquery.flot.tooltip.js",
              "src/flot/jquery.flot.selection.js",
              "src/flot/jquery.flot.canvas.js",
              "src/flot/jquery.flot.highlight.js",
              "node_modules/@claviska/jquery-minicolors/jquery.minicolors.js",
              "node_modules/html2canvas/dist/html2canvas.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "aws": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.aws.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production"
            },
            "local": {
              "browserTarget": "frontend:build:local"
            },
            "aws": {
              "browserTarget": "frontend:build:aws"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "scripts": [
              "src/flot/jquery.js",
              "src/flot/jquery.mousewheel.js",
              "src/flot/jquery.flot.js",
              "src/flot/jquery.flot.resize.js",
              "src/flot/jquery.flot.navigate.js",
              "src/flot/jquery.flot.fillarea.js",
              "src/flot/jquery.flot.crosshair.js",
              "src/flot/jquery.flot.time.js",
              "src/flot/jquery.flot.tooltip.js",
              "src/flot/jquery.flot.selection.js",
              "src/flot/jquery.flot.canvas.js",
              "src/flot/jquery.flot.highlight.js",
              "node_modules/@claviska/jquery-minicolors/jquery.minicolors.js",
              "node_modules/html2canvas/dist/html2canvas.js"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@claviska/jquery-minicolors/jquery.minicolors.css",
              "node_modules/angular-tree-component/dist/angular-tree-component.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "node_modules/@claviska/jquery-minicolors/jquery.minicolors.png"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "frontend-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "frontend:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "frontend",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}